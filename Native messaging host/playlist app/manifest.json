{
  "manifest_version": 2,
  "name": "Sound playing",
  "description": "For playlist app",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
	"tabs",
	"nativeMessaging",
	"background"
  ],
  "background": {
  "matches": ["<all_urls>"],
  "scripts": ["check_sound.js"]
  }
}
